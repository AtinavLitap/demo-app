pipeline {
	agent any
	environment {
		MY_CREDS = credentials("jenkinsID")
	}

	stages {
	  stage("stage1") {
	    steps {
		   withCredentials([
			usernamePassword(credentials: 'jenkinsID', usernameVariable: USER, passwordVariable: PWD)
		   ])

		   echo "MY_CREDS1 = USER: ${USER}, PWD: ${PWD}"
		   echo "MY_CREDS2 = ${MY_CREDS}"
	    }
          }
       }


}
