pipeline {
	agent any
	environment {
		MY_CREDS = credentials("jenkinsID")
	}

	stages {
	    stage("stage1") {
	        steps {
		        withCredentials([ usernamePassword(credentials: 'jenkinsID', usernameVariable: USER, passwordVariable: PWD) ]) {
		   		    echo "MY_CREDS2 = USER ${USER}, PWD ${PWD}"
			    }
	        }
       }
    }

}
